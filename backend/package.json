    "babel-loader": "^8.0.0-beta.0",
    "bcrypt": "^5.0.0",
    "body-parser": "^1.19.0",
    "cluster": "^0.7.7",
    "cors": "^2.8.5",
    "dotenv": "^8.0.0",
    "express": "^4.16.4",
    "express-joi-validator": "^2.0.1",
    "express-validation": "^1.0.2",
    "http": "0.0.0",
    "jest": "^24.8.0",
    "joi": "^14.3.1",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.20",
